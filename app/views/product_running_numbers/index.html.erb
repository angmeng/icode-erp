<h1>Listing product_running_numbers</h1>

<table>
  <tr>
    <th>Base name</th>
    <th>Sub name</th>
    <th>Running no</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_running_numbers.each do |product_running_number| %>
  <tr>
    <td><%= product_running_number.base_name %></td>
    <td><%= product_running_number.sub_name %></td>
    <td><%= product_running_number.running_no %></td>
    <td><%= link_to 'Show', product_running_number %></td>
    <td><%= link_to 'Edit', edit_product_running_number_path(product_running_number) %></td>
    <td><%= link_to 'Destroy', product_running_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

