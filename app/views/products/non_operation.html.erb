<div id="popup_container">
  <%= render "non_operation_top_icon", :data => @listing_categories, :match_field => @field_id %><br/><br/>

  <div id="product_layout" class="easyui-layout" style="width: 100%; height:500px;"> 
    <div region="west" split="true" title="Non Operation Product Categories" style="width:300px;">
      <%= render "/products/category", :categories => @categories %>
    </div>
    <div region="center" title="Non Operation Product Details" style="padding:5px;background:#eee; width: 100%;">
      <div id = "loading_products">
        <%= render "/products/product_details", :data => @listing_categories, :field_id => @field_id %>
      </div>
    </div> 
  </div> 

  <div class="icon_tag">
    <%= link_close_button %>
  </div>
</div>

