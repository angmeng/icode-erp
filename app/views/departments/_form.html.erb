<%= form_for(@department) do |f| %>
  <div class="fix_800x450">
    <table>
      <tr>
        <td width="100">Name *</td>
        <td><%= f.text_field :name, :class => "k-textbox", "required validationMessage" => "Please Enter Name" %></td>
