<table id="tabify_datatable" class="zebraLight display">
  <thead>
    <tr class="header_line">
      <th class="check_highlight"></th>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% dep.each do |department| %>
      <tr class="<%= cycle('odd_line', 'even_line') %>" controllerName="<%= controller_name %>" rowsId="<%= department.id %>">
        <td></td>
        <td><%= department.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>