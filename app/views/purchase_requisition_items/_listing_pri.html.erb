<h1>Listing purchase_requisition_items</h1>

<table>
  <tr>
    <th>Purchase requisition</th>
    <th>Product</th>
    <th>Trade company</th>
    <th>Description</th>
    <th>Quantity</th>
    <th>Unit measurement</th>
    <th>Eta</th>
    <th>Purchase order no</th>
    <th>Unit price</th>
    <th>Remark</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% pri.each do |purchase_requisition_item| %>
  <tr>
    <td><%= purchase_requisition_item.purchase_requisition_id %></td>
    <td><%= purchase_requisition_item.product_id %></td>
    <td><%= purchase_requisition_item.trade_company_id %></td>
    <td><%= purchase_requisition_item.description %></td>
    <td><%= purchase_requisition_item.quantity %></td>
    <td><%= purchase_requisition_item.unit_measurement_id %></td>
    <td><%= purchase_requisition_item.eta %></td>
    <td><%= purchase_requisition_item.purchase_order_no %></td>
    <td><%= purchase_requisition_item.unit_price %></td>
    <td><%= purchase_requisition_item.remark %></td>
    <td><%= link_to 'Show', purchase_requisition_item %></td>
    <td><%= link_to 'Edit', edit_purchase_requisition_item_path(purchase_requisition_item) %></td>
    <td><%= link_to 'Destroy', purchase_requisition_item, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>
