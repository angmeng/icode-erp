<%#*<table>%>
  <% if checkboxes.present? %>
    <% if action_name == "show" %>
  
      <tr>
        <td></td>
        <td><%= ims_show_disabled(checkboxes, InventoryManagementSystem::INVENTORY_MENU) %> Inventories</td>
        <td><%= ims_show_disabled(checkboxes, InventoryManagementSystem::INVENTORY_TRANSACTION) %> Inventories Data</td>
        <td align="center"><%= ims_show_disabled(checkboxes, InventoryManagementSystem::INVENTORY_NEW) %></td>
        <td align="center"><%= ims_show_disabled(checkboxes, InventoryManagementSystem::INVENTORY_TRANSACTION_SHOW) %></td>
        <td align="center"><%= ims_show_disabled(checkboxes, InventoryManagementSystem::INVENTORY_TRANSACTION_EDIT) %></td>
        <td align="center"><%= ims_show_disabled(checkboxes, InventoryManagementSystem::INVENTORY_TRANSACTION_KIV) %></td>
        <td align="center"><%= ims_show_disabled(checkboxes, InventoryManagementSystem::INVENTORY_TRANSACTION_PRINTABLE) %></td>
        <% 2.times do |td| %><td></td><% end %>
      </tr>
  
    <% else %>
  
      <tr>
        <td></td>
        <td><%= ims_edit_checked(checkboxes, InventoryManagementSystem::INVENTORY_MENU) %> Inventories</td>
        <td><%= ims_edit_checked(checkboxes, InventoryManagementSystem::INVENTORY_TRANSACTION) %> Inventories Data</td>
        <td align="center"><%= ims_edit_checked(checkboxes, InventoryManagementSystem::INVENTORY_NEW) %></td>
        <td align="center"><%= ims_edit_checked(checkboxes, InventoryManagementSystem::INVENTORY_TRANSACTION_SHOW) %></td>
        <td align="center"><%= ims_edit_checked(checkboxes, InventoryManagementSystem::INVENTORY_TRANSACTION_EDIT) %></td>
        <td align="center"><%= ims_edit_checked(checkboxes, InventoryManagementSystem::INVENTORY_TRANSACTION_KIV) %></td>
        <td align="center"><%= ims_edit_checked(checkboxes, InventoryManagementSystem::INVENTORY_TRANSACTION_PRINTABLE) %></td>
        <% 2.times do |td| %><td></td><% end %>
      </tr>
      
    <% end %>

  <% else %>
    
      <tr>
        <td></td>
        <td><%= ims_new(InventoryManagementSystem::INVENTORY_MENU) %> Inventories</td>
        <td><%= ims_new(InventoryManagementSystem::INVENTORY_TRANSACTION) %> Inventories Data</td>
        <td align="center"><%= ims_new(InventoryManagementSystem::INVENTORY_NEW) %></td>
        <td align="center"><%= ims_new(InventoryManagementSystem::INVENTORY_TRANSACTION_SHOW) %></td>
        <td align="center"><%= ims_new(InventoryManagementSystem::INVENTORY_TRANSACTION_EDIT) %></td>
        <td align="center"><%= ims_new(InventoryManagementSystem::INVENTORY_TRANSACTION_KIV) %></td>
        <td align="center"><%= ims_new(InventoryManagementSystem::INVENTORY_TRANSACTION_PRINTABLE) %></td>
        <% 2.times do |td| %><td></td><% end %>
      </tr>

  <% end %>
<%#*</table>%>