        <td align="center"><%= check_box_tag "inventory_management_system[]", InventoryManagementSystem::RECEIVE_NOTE_COMPLETED_VIEW_SHOW, checkboxes.include?(InventoryManagementSystem::RECEIVE_NOTE_COMPLETED_VIEW_SHOW) %></td>
        <% 9.times do |td| %><td></td><% end %>
      </tr>
      
    <% end %>
    
  <% else %>
    
    <tr>
      <td></td>
      <td><%= check_box_tag "inventory_management_system[]", InventoryManagementSystem::RECEIVE_NOTE_MENU %> RECEIVE NOTE</td>
      <td><%= check_box_tag "inventory_management_system[]", InventoryManagementSystem::RECEIVE_NOTE_NEW %> New RNs</td>
      <td align="center"><%= check_box_tag "inventory_management_system[]", InventoryManagementSystem::RECEIVE_NOTE_NEW_ADD %></td>
      <td><%#= "save" %></td>
      <% 10.times do |td| %><td></td><% end %>
    </tr>
    
    <tr>
      <td></td>
      <td></td>
      <td><%= check_box_tag "inventory_management_system[]", InventoryManagementSystem::RECEIVE_NOTE_TRANSACTION %> RN Transactions & Histories</td>
      <td></td><td><%#= "save" %></td>
      <td align="center"><%= check_box_tag "inventory_management_system[]", InventoryManagementSystem::RECEIVE_NOTE_TRANSACTION_SHOW %></td>
      <td align="center"><%= check_box_tag "inventory_management_system[]", InventoryManagementSystem::RECEIVE_NOTE_TRANSACTION_EDIT %></td>
      <td align="center"><%= check_box_tag "inventory_management_system[]", InventoryManagementSystem::RECEIVE_NOTE_TRANSACTION_KIV %></td>
      <td align="center"><%= check_box_tag "inventory_management_system[]", InventoryManagementSystem::RECEIVE_NOTE_TRANSACTION_PRINTABLE %></td>
      <% 6.times do |td| %><td></td><% end %>
    </tr>
    
    <tr>
      <td></td>
      <td></td>
      <td><%= check_box_tag "inventory_management_system[]", InventoryManagementSystem::RECEIVE_NOTE_COMPLETED_VIEW %> RN Completed View</td>
      <td></td><td><%#= "save" %></td>
      <td align="center"><%= check_box_tag "inventory_management_system[]", InventoryManagementSystem::RECEIVE_NOTE_COMPLETED_VIEW_SHOW %></td>
      <% 9.times do |td| %><td></td><% end %>
    </tr>
    
  <% end %>
<%#*</table>%>