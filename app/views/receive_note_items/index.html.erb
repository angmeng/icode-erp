<h1>Listing receive_note_items</h1>

<table>
  <tr>
    <th>Receive note</th>
    <th>Purchase order</th>
    <th>Received quantity</th>
    <th>Status</th>
    <th>Order quantity</th>
    <th>Balance quantity</th>
    <th>Current stock</th>
    <th>Vendor part</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @receive_note_items.each do |receive_note_item| %>
  <tr>
    <td><%= receive_note_item.receive_note_id %></td>
    <td><%= receive_note_item.purchase_order_id %></td>
    <td><%= receive_note_item.received_quantity %></td>
    <td><%= receive_note_item.status %></td>
    <td><%= receive_note_item.order_quantity %></td>
    <td><%= receive_note_item.balance_quantity %></td>
    <td><%= receive_note_item.current_stock %></td>
    <td><%= receive_note_item.vendor_part %></td>
    <td><%= link_to 'Show', receive_note_item %></td>
    <td><%= link_to 'Edit', edit_receive_note_item_path(receive_note_item) %></td>
    <td><%= link_to 'Destroy', receive_note_item, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

