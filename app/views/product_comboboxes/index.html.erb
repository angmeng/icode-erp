<h1>Listing product_comboboxes</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Product code</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_comboboxes.each do |product_combobox| %>
  <tr>
    <td><%= product_combobox.product_id %></td>
    <td><%= product_combobox.product_code %></td>
    <td><%= link_to 'Show', product_combobox %></td>
    <td><%= link_to 'Edit', edit_product_combobox_path(product_combobox) %></td>
    <td><%= link_to 'Destroy', product_combobox, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Product combobox', new_product_combobox_path %>
