<%#*<table id="grid_userTable_h490" class="normal_table_black zebraLight">%>
<table id="ims_purchase" class="normal_table_black zebraLight">
  <thead>
    <tr class="header_line">
      <th class="check_highlight"></th>
      <th width="150">MODULE</th>
      <th width="200">SUB-MODULE</th>
      <th width="50">Add or Exist Files</th>
      <th width="50">Save</th>
      <th width="50">Show</th>
      <th width="50">Edit</th>
      <th width="50">Drop To KIV</th>
      <th width="50">Printer</th>
      <th width="50">Show KIV</th>
      <th width="50">Recover</th>
      <th width="50">Apply</th>
      <th width="50">Submit to</th>
      <th width="50">Send Mail</th>
      <th width="50">Costing Sheet</th>
    </tr>
  </thead>
  
  <tbody>
    <!--PURCHASE-->
    <%= render "/inventory_management_systems/ims_purchase_requisition", :checkboxes => @checkboxes %>
    <%= render "/inventory_management_systems/ims_purchase_order", :checkboxes => @checkboxes %>
    <%= render "/inventory_management_systems/ims_receive_note", :checkboxes => @checkboxes %>
    <!--SALES-->
    <%= render "/inventory_management_systems/ims_quotation_requisition", :checkboxes => @checkboxes %>
    <%= render "/inventory_management_systems/ims_sales_order", :checkboxes => @checkboxes %>
    <%= render "/inventory_management_systems/ims_delivery_order", :checkboxes => @checkboxes %>
    <!--OTHER-->
    <%= render "/inventory_management_systems/ims_sales_tax_exemption", :checkboxes => @checkboxes %>
    <%= render "/inventory_management_systems/ims_formulation_for_all", :checkboxes => @checkboxes %>
    <%= render "/inventory_management_systems/ims_formulation_for_costing", :checkboxes => @checkboxes %>
    <%= render "/inventory_management_systems/ims_inventory", :checkboxes => @checkboxes %>
    <%= render "/inventory_management_systems/ims_company_profile", :checkboxes => @checkboxes %>
    <%= render "/inventory_management_systems/ims_trade_company", :checkboxes => @checkboxes %>
    <%= render "/inventory_management_systems/ims_price_control", :checkboxes => @checkboxes %>
    <!-- ACCOUNT -->
    <%#= render "/inventory_management_systems/ims_account", :checkboxes => @checkboxes %>
  </tbody>
</table>
